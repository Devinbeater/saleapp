{
  "name": "daily-sheet-manager",
  "version": "1.1.0",
  "description": "Daily Sheet Manager with HyperFormula integration and TestSprite testing",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "setup-db": "node server/scripts/setup-database.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:sprite": "npx @testsprite/testsprite-mcp generateCodeAndExecute",
    "test:backend": "cd server && npm test",
    "test:frontend": "cd client && npm test",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:unit": "npm run test:backend && npm run test:frontend",
    "test:integration": "npm run test:sprite",
    "start": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "devDependencies": {
    "@testsprite/testsprite-mcp": "^0.0.14",
    "jest": "^30.2.0"
  },
  "keywords": [
    "spreadsheet",
    "daily-sheet",
    "hyperformula",
    "postgresql",
    "testing",
    "testsprite"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  }
}
